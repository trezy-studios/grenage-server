{
  "name": "grenage-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "start-containers": "yarn start-database-containers && yarn start-app-container",
    "migrate": "knex migrate:latest --env development",
    "seed": "knex seed:run --env development",
    "start": "node dist/index.js",
    "start-app-container": "docker-compose up app",
    "start-database-containers": "docker-compose up -d postgres redis",
    "test": "mocha --recursive --require @babel/register"
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@koa/cors": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "dotenv": "^7.0.0",
    "faker": "^4.1.0",
    "knex": "^0.16.3",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-no-trailing-slash": "^2.1.0",
    "koa-passport": "^4.1.1",
    "koa-qs": "^2.0.0",
    "koa-redis": "^3.1.3",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-session": "^5.10.1",
    "mocha": "^6.1.0",
    "mock-require": "^3.0.3",
    "nodemailer": "^6.1.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0",
    "sequelize": "^5.2.12",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "workerpool": "^3.1.2",
    "yayson": "^2.0.8"
  }
}
